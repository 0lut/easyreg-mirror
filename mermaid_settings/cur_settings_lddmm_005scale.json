{
    "model": {
        "deformation": {
            "compute_similarity_measure_at_low_res": false, 
            "map_low_res_factor": 0.5,
            "use_map": true
        }, 
        "registration_model": {
            "forward_model": {
                "number_of_time_steps": 10,
                "solver": "rk4",
                "adjoin_on": true,
                "rtol": 1e-5,
                "atol": 1e-5,
                "smoother": {
                    "multi_gaussian_stds": [
                        0.05
                    ],
                    "multi_gaussian_weights": [
                        1.0
                    ], 
                    "type": "multiGaussian"
                }
            }, 
            "loss": {
                "display_max_displacement": false, 
                "limit_displacement": false, 
                "max_displacement": 0.05
            }, 
            "similarity_measure": {
                "develop_mod_on": false,
                "sigma": 1.0,
                "type": "lncc",
                "lncc":{
                    "resol_bound":[-1],
                    "kernel_size_ratio":[[0.25]],
                    "kernel_weight_ratio":[[1.0]],
                    "stride":[0.25],
                    "dilation":[1]
                }
            },
            "load_velocity_from_forward_model":true,
            "spline_order": 1, 
            "type": "lddmm_shooting_map"
        }
    }, 
    "optimizer": {
        "lbfgs": {
            "history_size": 5, 
            "line_search_fn": "backtracking", 
            "max_eval": 5, 
            "max_iter": 1
        }, 
        "multi_scale": {
            "scale_factors": [
                1.0,
                0.5,
                0.25
            ],
            "scale_iterations": [
                50,
                50,
                50
            ]
        }, 
        "name": "lbfgs_ls", 
        "scheduler": {
            "factor": 0.5, 
            "patience": 10, 
            "verbose": true
        }, 
        "single_scale": {
            "nr_of_iterations": 100, 
            "rel_ftol": 1e-09
        }, 
        "use_step_size_scheduler": true
    }
}